<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wcj.mapper.CollectionMapper">
    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.wcj.pojo.BlogCollection">
        <id column="collection_id" property="collectionId"/>
        <result column="blog_id" property="blogId"/>
        <result column="user_id" property="userId"/>
        <result column="collection_time" property="collectionTime"/>

    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        collection_id,
        blog_id,
        user_id,
        collection_time
    </sql>

</mapper>
